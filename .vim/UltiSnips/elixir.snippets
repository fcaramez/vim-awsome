snippet f@ "Function with optional arguments"
def ${1:function_name}(${2:opts} \\ ${3:[]}) do
	${4:# function body}
end
endsnippet

snippet t@ "Test case"
test "${1:test_case}" do
	${2:# test body}
end
endsnippet

snippet m@ "New Module"
defmodule ${1:module_name} do
	${2:# module body}
end
endsnippet
